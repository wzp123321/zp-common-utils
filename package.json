{
  "name": "zp-common-utils",
  "type": "module",
  "author": {
    "name": "zpwan",
    "url": "https://github.com/wzp123321"
  },
  "description": "Vue Utils",
  "public": true,
  "version": "0.2.4",
  "files": [
    "lib"
  ],
  "main": "./lib/zp-common-utils.umd.cjs",
  "module": "./lib/zp-common-utils.js",
  "typings": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/zp-common-utils.js",
      "require": "./lib/zp-common-utils.umd.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "keywords": [
    "vue",
    "vite",
    "utils",
    "typescript",
    "lib",
    "npm"
  ],
  "scripts": {
    "dev": "vite",
    "update": "esno scripts/update.ts",
    "build": "npm run update && vue-tsc && vite build",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "esbuild-register": "^3.5.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.9.1",
    "vue-tsc": "^2.0.6"
  },
  "repository": {
    "url": "https://github.com/wzp123321/zp-common-utils.git"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
